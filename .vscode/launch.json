{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI Debug (Uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.app_module:app",
        "--reload"
      ],
      "cwd": "${workspaceFolder}/src",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
